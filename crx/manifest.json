{
	"manifest_version": 3,
	"name": "dolphins",
	"version": "1.0.0",
	"description": "",
	"author": "yuanlin-yu",
	"action":
	{
		"default_icon": {
			"48": "icon.png",
			"128": "icon.png"
		},
		"default_popup": "popup.html"
	},
	"permissions": [
		"activeTab", 
		"scripting", 
		"downloads"
	],
	"content_scripts": 
	[
		{
			"matches": ["<all_urls>"],
			"js": ["dist/dolphins.min.js"],
			"isolation": "split"
		}
	],
	"background": {
		"service_worker": "background.js"
	},
	"web_accessible_resources": 
	[

	]
}